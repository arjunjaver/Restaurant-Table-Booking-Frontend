{
  "version": 2,
  "builds": [
    {
      "src": "app/page.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/page.js"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://restaurant-table-booking-backend-liard.vercel.app/"
  }
}
